{"remainingRequest":"D:\\vue\\node_modules\\@vue\\cli-service\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\vue\\src\\views\\system\\SysFillRuleList.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\vue\\src\\views\\system\\SysFillRuleList.vue","mtime":1615948822102},{"path":"D:\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1615952641464},{"path":"D:\\vue\\node_modules\\babel-loader\\lib\\index.js","mtime":1615952610415},{"path":"D:\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1615952641464},{"path":"D:\\vue\\node_modules\\@vue\\cli-service\\node_modules\\vue-loader\\lib\\index.js","mtime":1615952643260}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["SysFillRuleList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmGA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SysFillRuleList.vue","sourceRoot":"src/views/system","sourcesContent":["<template>\r\n  <a-card :bordered=\"false\">\r\n\r\n    <!-- 查询区域 -->\r\n    <div class=\"table-page-search-wrapper\">\r\n      <a-form layout=\"inline\" @keyup.enter.native=\"searchQuery\">\r\n        <a-row :gutter=\"24\">\r\n          <a-col :md=\"6\" :sm=\"8\">\r\n            <a-form-item label=\"规则名称\">\r\n              <a-input placeholder=\"请输入规则名称\" v-model=\"queryParam.ruleName\"></a-input>\r\n            </a-form-item>\r\n          </a-col>\r\n          <a-col :md=\"6\" :sm=\"8\">\r\n            <a-form-item label=\"规则Code\">\r\n              <a-input placeholder=\"请输入规则Code\" v-model=\"queryParam.ruleCode\"></a-input>\r\n            </a-form-item>\r\n          </a-col>\r\n          <a-col :md=\"6\" :sm=\"8\">\r\n            <span style=\"float: left;overflow: hidden;\" class=\"table-page-search-submitButtons\">\r\n              <a-button type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\r\n              <a-button type=\"primary\" @click=\"searchReset\" icon=\"reload\" style=\"margin-left: 8px\">重置</a-button>\r\n            </span>\r\n          </a-col>\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n\r\n    <!-- 操作按钮区域 -->\r\n    <div class=\"table-operator\">\r\n      <a-button @click=\"handleAdd\" type=\"primary\" icon=\"plus\">新增</a-button>\r\n      <a-button type=\"primary\" icon=\"download\" @click=\"handleExportXls('填值规则')\">导出</a-button>\r\n      <a-upload name=\"file\" :showUploadList=\"false\" :multiple=\"false\" :headers=\"tokenHeader\" :action=\"importExcelUrl\" @change=\"handleImportExcel\">\r\n        <a-button type=\"primary\" icon=\"import\">导入</a-button>\r\n      </a-upload>\r\n      <a-dropdown v-if=\"selectedRowKeys.length > 0\">\r\n        <a-menu slot=\"overlay\">\r\n          <a-menu-item key=\"1\" @click=\"batchDel\">\r\n            <a-icon type=\"delete\"/>\r\n            删除\r\n          </a-menu-item>\r\n        </a-menu>\r\n        <a-button style=\"margin-left: 8px\"> 批量操作\r\n          <a-icon type=\"down\"/>\r\n        </a-button>\r\n      </a-dropdown>\r\n    </div>\r\n\r\n    <!-- table区域-begin -->\r\n    <a-alert type=\"info\" showIcon style=\"margin-bottom: 16px;\">\r\n      <template slot=\"message\">\r\n        <span>已选择</span>\r\n        <a style=\"font-weight: 600;padding: 0 4px;\">{{ selectedRowKeys.length }}</a>\r\n        <span>项</span>\r\n        <template v-if=\"selectedRowKeys.length>0\">\r\n          <a-divider type=\"vertical\"/>\r\n          <a @click=\"onClearSelected\">清空</a>\r\n        </template>\r\n      </template>\r\n    </a-alert>\r\n\r\n    <a-table\r\n      ref=\"table\"\r\n      size=\"middle\"\r\n      bordered\r\n      rowKey=\"id\"\r\n      :columns=\"columns\"\r\n      :dataSource=\"dataSource\"\r\n      :pagination=\"ipagination\"\r\n      :loading=\"loading\"\r\n      :rowSelection=\"{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}\"\r\n      @change=\"handleTableChange\">\r\n\r\n        <span slot=\"action\" slot-scope=\"text, record\">\r\n          <a @click=\"handleEdit(record)\">编辑</a>\r\n          <a-divider type=\"vertical\"/>\r\n          <a-dropdown>\r\n            <a class=\"ant-dropdown-link\">更多 <a-icon type=\"down\"/></a>\r\n            <a-menu slot=\"overlay\">\r\n              <a-menu-item @click=\"handleTest(record)\">\r\n                功能测试\r\n              </a-menu-item>\r\n              <a-menu-item>\r\n                <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record.id)\">\r\n                  <a>删除</a>\r\n                </a-popconfirm>\r\n              </a-menu-item>\r\n            </a-menu>\r\n          </a-dropdown>\r\n        </span>\r\n\r\n    </a-table>\r\n    <!-- table区域-end -->\r\n\r\n    <!-- 表单区域 -->\r\n    <sys-fill-rule-modal ref=\"modalForm\" @ok=\"modalFormOk\"/>\r\n  </a-card>\r\n</template>\r\n\r\n<script>\r\n  import { getAction } from '@/api/manage'\r\n  import SysFillRuleModal from './modules/SysFillRuleModal'\r\n  import { JeecgListMixin } from '@/mixins/JeecgListMixin'\r\n\r\n  export default {\r\n    name: 'SysFillRuleList',\r\n    mixins: [JeecgListMixin],\r\n    components: { SysFillRuleModal },\r\n    data() {\r\n      return {\r\n        description: '填值规则管理页面',\r\n        // 表头\r\n        columns: [\r\n          {\r\n            title: '#',\r\n            dataIndex: '',\r\n            key: 'rowIndex',\r\n            width: 60,\r\n            align: 'center',\r\n            customRender: (t, r, index) => 1 + index\r\n          },\r\n          {\r\n            title: '规则名称',\r\n            align: 'center',\r\n            dataIndex: 'ruleName'\r\n          },\r\n          {\r\n            title: '规则Code',\r\n            align: 'center',\r\n            dataIndex: 'ruleCode'\r\n          },\r\n          {\r\n            title: '规则实现类',\r\n            align: 'center',\r\n            dataIndex: 'ruleClass'\r\n          },\r\n          {\r\n            title: '规则参数',\r\n            align: 'center',\r\n            dataIndex: 'ruleParams'\r\n          },\r\n          {\r\n            title: '操作',\r\n            dataIndex: 'action',\r\n            align: 'center',\r\n            scopedSlots: { customRender: 'action' },\r\n          }\r\n        ],\r\n        url: {\r\n          list: '/sys/fillRule/list',\r\n          test: '/sys/fillRule/testFillRule',\r\n          delete: '/sys/fillRule/delete',\r\n          deleteBatch: '/sys/fillRule/deleteBatch',\r\n          exportXlsUrl: '/sys/fillRule/exportXls',\r\n          importExcelUrl: '/sys/fillRule/importExcel',\r\n        },\r\n      }\r\n    },\r\n    computed: {\r\n      importExcelUrl() {\r\n        return `${window._CONFIG['domianURL']}${this.url.importExcelUrl}`\r\n      }\r\n    },\r\n    methods: {\r\n      handleTest(record) {\r\n        let closeLoading = this.$message.loading('生成中...', 0)\r\n\r\n        getAction(this.url.test, {\r\n          ruleCode: record.ruleCode\r\n        }).then(res => {\r\n          if (res.success) {\r\n            this.$info({\r\n              title: '填值规则功能测试',\r\n              content: '生成结果：' + res.result\r\n            })\r\n          } else {\r\n            this.$message.warn(res.message)\r\n          }\r\n        }).finally(() => {\r\n          closeLoading()\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style scoped>\r\n  @import '~@assets/less/common.less'\r\n</style>"]}]}