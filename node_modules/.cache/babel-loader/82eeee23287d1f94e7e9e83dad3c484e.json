    <span>{{ rate }}%</span>\r\n    <span :class=\"['trend-icon', trend]\"><a-icon :type=\"'caret-' + trend\"/></span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: \"Trend\",\r\n    props: {\r\n      // 同title\r\n      term: {\r\n        type: String,\r\n        default: '',\r\n        required: true\r\n      },\r\n      // 百分比\r\n      percentage: {\r\n        type: Number,\r\n        default: null\r\n      },\r\n      type: {\r\n        type: Boolean,\r\n        default: null\r\n      },\r\n      target: {\r\n        type: Number,\r\n        default: 0\r\n      },\r\n      value: {\r\n        type: Number,\r\n        default: 0\r\n      },\r\n      fixed: {\r\n        type: Number,\r\n        default: 2\r\n      }\r\n    },\r\n    data () {\r\n      return {\r\n        trend: this.type && 'up' || 'down',\r\n        rate: this.percentage\r\n      }\r\n    },\r\n    created () {\r\n      let type = this.type === null ? this.value >= this.target : this.type\r\n      this.trend = type ? 'up' : 'down';\r\n      this.rate = (this.percentage === null ? Math.abs(this.value - this.target) * 100 / this.target : this.percentage).toFixed(this.fixed)\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .chart-trend {\r\n    display: inline-block;\r\n    font-size: 14px;\r\n    line-height: 22px;\r\n\r\n    .trend-icon {\r\n      font-size: 12px;\r\n\r\n      &.up, &.down {\r\n        margin-left: 4px;\r\n        position: relative;\r\n        top: 1px;\r\n\r\n        i {\r\n          font-size: 12px;\r\n          transform: scale(.83);\r\n        }\r\n      }\r\n\r\n      &.up {\r\n        color: #f5222d;\r\n      }\r\n      &.down {\r\n        color: #52c41a;\r\n        top: -1px;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/chart"}]}