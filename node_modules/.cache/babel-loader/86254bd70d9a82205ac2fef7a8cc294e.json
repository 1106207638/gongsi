rns {string}\r\n */\r\nexport const cutStrByFullLength = (str = '', maxLength) => {\r\n  let showLength = 0\r\n  return str.split('').reduce((pre, cur) => {\r\n    const charCode = cur.charCodeAt(0)\r\n    if (charCode >= 0 && charCode <= 128) {\r\n      showLength += 1\r\n    } else {\r\n      showLength += 2\r\n    }\r\n    if (showLength <= maxLength) {\r\n      return pre + cur\r\n    }\r\n    return pre\r\n  }, '')\r\n}"]}]}