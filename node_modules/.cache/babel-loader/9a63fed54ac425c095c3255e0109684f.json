\"getTitle\" :link-list=\"linkList\" :search=\"search\" :tabs=\"tabs\">\r\n    <div slot=\"extra\" class=\"extra-img\">\r\n      <img :src=\"extraImage\"/>\r\n    </div>\r\n    <!-- keep-alive  -->\r\n    <route-view ref=\"content\"></route-view>\r\n  </page-layout>\r\n</template>\r\n\r\n<script>\r\n  import PageLayout from '../page/PageLayout'\r\n  import RouteView from './RouteView'\r\n\r\n  export default {\r\n    name: \"PageContent\",\r\n    components: {\r\n      RouteView,\r\n      PageLayout\r\n    },\r\n    data () {\r\n      return {\r\n        title: '',\r\n        description: '',\r\n        linkList: [],\r\n        extraImage: '',\r\n        search: false,\r\n        tabs: {}\r\n      }\r\n    },\r\n    mounted () {\r\n      this.getPageHeaderInfo()\r\n    },\r\n    updated () {\r\n      this.getPageHeaderInfo()\r\n    },\r\n    computed: {\r\n\r\n      getTitle () {\r\n        return this.$route.meta.title\r\n      }\r\n\r\n    },\r\n    methods: {\r\n      getPageHeaderInfo () {\r\n        // eslint-disable-next-line\r\n        this.title = this.$route.meta.title\r\n        // 因为套用了一层 route-view 所以要取 ref 对象下的子节点的第一个对象\r\n        const content = this.$refs.content && this.$refs.content.$children[0]\r\n\r\n        if (content) {\r\n          this.description = content.description\r\n          this.linkList = content.linkList\r\n          this.extraImage = content.extraImage\r\n          this.search = content.search == true ? true : false\r\n          this.tabs = content.tabs\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .extra-img {\r\n    margin-top: -60px;\r\n    text-align: center;\r\n    width: 195px;\r\n\r\n    img {\r\n      width: 100%;\r\n    }\r\n  }\r\n\r\n  .mobile {\r\n    .extra-img{\r\n      margin-top: 0;\r\n      text-align: center;\r\n      width: 96px;\r\n\r\n      img{\r\n        width: 100%;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/layouts"}]}